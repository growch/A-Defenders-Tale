package util {	import flash.net.URLRequest;		import flash.net.navigateToURL;	/**	 * @author iasseo	 */	public class GetURL {		public static function to(url:String, window:String = "_self"):void        {            var req:URLRequest = new URLRequest(url);            try            {                navigateToURL(req, window);            }            catch (e:Error)            {                trace("Navigate to URL failed", e.message);            }        }	}}